{
	"name": "car",
	"description": "Retrieve OCDLA Case Reviews.",
	"comment": "Module definition file.",
	"connectedApp": "default",
	"mail": true,
	"files": ["Model.php","Mail.php"],
	"links": [
		{
			"href":"/car/new",
			"label": "Add Case Review",
			"isSublink": true,
			"adminLink": true,
			"parentLinkId":"car"
		}
	],
	"routes": {
		"car/list/%id":{
			"callback":"getList",
			"content-type":"text\/html"
		},

		"car/list":{
			"callback":"getList",
			"content-type":"text\/html"
		},
		"car/flag":{
			"callback":"flag",
			"content-type":"application/json",
			"access":"is_authenticated",
			"authorization":"webserver"
		},
		"car/new":{
			"callback":"showRecordForm",
			"content-type":"text/html",
			"access":"is_admin",
			"authorization":"webserver"
		},
		"car/edit/%id":{
			"callback":"showRecordForm",
			"content-type":"text/html",
			"access":"is_admin",
			"authorization":"webserver"
		},
		"car/save":{
			"callback":"save",
			"content-type":"text/html",
			"access":"is_admin",
			"authorization":"webserver"
		},
		"car/delete/%id":{
			"callback":"delete",
			"content-type":"text/html",
			"access":"is_admin",
			"authorization":"webserver"
		}
	}
}